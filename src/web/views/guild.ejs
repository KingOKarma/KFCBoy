<!DOCTYPE html>
<html>
  <head>
    <title>Global xp</title>
    <meta http-equiv="Refresh" content="300" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta
      name="description"
      content="this is the global leaderboard for KFC bucket boy"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <link rel="stylesheet" href="/stylesheets/nav.css" />
  </head>
  <body>
    <div class="topnav">
      <label for="home" style="text-decoration: underline; font-weight: bold; font-size: xx-large; margin-left: 3vw;">KFC Bucket boy</label>
      <a href="/" class="active" id="home" id="home">Home</a>
      <a href="/dashboard">Dashboard</a>
      <a href="/leaderboards">Leaderboards</a>
      <a href="/devs">Devs</a>
      <a href="/donate">Donate</a>
      <a href="/contact">Contact</a>
      <a href="/about">About</a>
  </div>

    <table>
      <thead>
        <tr>
          <th></th>
          <th>Name</th>
          <th>UserID</th>
          <th>level</th>
          <th>xp</th>
        </tr>
      </thead>
      <tbody>
        <% var count = 0; %> <% users.forEach(user => { %>
        <tr>
          <td>
            <img src="<%= user.AvatarURL? user.AvatarURL :
            "https://www.online-tech-tips.com/wp-content/uploads/2019/09/discord.jpg"
            %> " alt="" width="64" height="64">
          </td>
          <td><h6><%= user.UserName %></h6></td>
          <td><h6><%= user.UserID %></h6></td>
          <td><h6><%= user.level %></h6></td>
          <td><h6><%= user.xp %></h6></td>
        </tr>
        <%}) %>
      </tbody>
    </table>
  </body>
</html>
